	/* chosen */
	chosen: chosen {
		bootargs = "console=tty0 root=/dev/ram \
			swiotlb=noforce \
			8250.nr_uarts=4 \
			initcall_debug=1 cma=64M transparent_hugepage=never \
			firmware_class.path=/vendor/firmware";
			kaslr-seed = <0 0>;
	};

