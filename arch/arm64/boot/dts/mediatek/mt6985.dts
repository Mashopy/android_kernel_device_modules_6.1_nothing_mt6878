		compatible = "mediatek,mt6985-gpusys", "syscon";
		reg = <0 0x13F91000 0 0x1000>;
		#power-domain-cells = <1>;
		ifr_bus = <&ifr_bus>;
		ufscfg_ao_bus = <&ufscfg_ao_clk>;
		gpu_eb_rpc = <&gpusys>;
