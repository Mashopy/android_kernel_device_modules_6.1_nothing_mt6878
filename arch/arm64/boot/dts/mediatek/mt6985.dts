			      <10 0x2000>, /* sensor debug */
			      <11 0x100>, /* sensor custom writer */
			      <12 0x100>, /* sensor custom reader */
			      <13 0x200000>; /* aov */

		memorydump = <0x300000>, /* l2tcm */
